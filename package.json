{
  "name": "electron-react-deribit",
  "version": "0.1.0",
  "private": true,
  "description": "Electron application for Deribit",
  "dependencies": {
    "electron-is-dev": "^1.0.1",
    "electron-log": "^2.2.17",
    "electron-store": "^2.0.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1",
    "update-electron-app": "^1.3.0"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test --env=jsdom",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "release": "yarn react-build && electron-builder --publish=always",
    "build": "yarn react-build && yarn electron-build",
    "start": "concurrently \"cross-env BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && electron .\"",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "electron-pack": "build -c.extraMetadata.main=public/electron.js",
    "preelectron-pack": "yarn build",
    "dist-all": "electron-builder -mwl"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "electron": "^3.0.10",
    "electron-builder": "^20.38.2",
    "wait-on": "^3.2.0"
  },
  "build  ": {
    "asar": false,
    "productName": "ElectronDeribitApp",
    "appId": "com.example.deribit",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "release"
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/public"
        }
      ]
    },
    "win": {
      "target": [
        "nsis",
        "msi"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "snap",
        "AppImage"
      ],
      "category": "Development"
    },
    "publish": {
      "provider": "github",
      "owner": "priyank57",
      "repo": "electron-demo",
      "private": false
    },
    "extends": null
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/priyankgohil/electron-demo"
  },
  "author": {
    "name": "Levaral",
    "email": "mail@levaral.com",
    "url": "https://levaral.com"
  },
  "devEngines": {
    "node": ">=7.x",
    "npm": ">=4.x",
    "yarn": ">=0.21.3"
  }
}
